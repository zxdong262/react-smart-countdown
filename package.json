{
  "name": "react-smart-countdown",
  "version": "0.0.1",
  "description": "smart react countdown component",
  "main": "dist/react-smart-countdown.js",
  "scripts": {
    "build": "rm dist/*.* && NODE_ENV=production ./node_modules/.bin/webpack -p --progress",
    "start": "./node_modules/.bin/npm-run-all -p dev:server dev:client ",
    "dev:server": "node build/dev-server",
    "dev:server-debug": "node --debug build/dev-server",
    "dev:client": "./node_modules/.bin/webpack-dev-server --progress --colors --inline --content-base dist/",
    "test": "./node_modules/.bin/karma start ./test/karma.conf.js"
  },
  "author": {
    "email": "zxdong@gmail.com",
    "name": "ZHAO Xudong",
    "url": "0.10.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zxdong262/react-smart-countdown"
  },
  "keywords": [
    "react",
    "countdown",
    "smart"
  ],
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-es2015-modules-factory": "6.0.0",
    "babel-plugin-transform-es2015-modules-umd": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "bootstrap": "4.0.0-alpha.5",
    "jquery": "3.1.1",
    "karma": "1.3.0",
    "karma-chai-plugins": "0.8.0",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "koa": "1.2.4",
    "koa-mount": "1.3.0",
    "koa-pug": "3.0.0-1",
    "koa-router": "5.4.0",
    "koa-static": "2.0.0",
    "mocha": "3.2.0",
